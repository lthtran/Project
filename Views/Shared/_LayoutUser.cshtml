    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
    <script>
    $(document).ready(function(){
        $("#SearchString").on('input', function(){
            var query = $(this).val();
            if(query.length > 1){
                $.ajax({
                    url: '@Url.Action("AutomationSearch", "User")',
                    type: 'GET',
                    data: { searchString: query },
                    success: function(data) {
                        var suggestions = $("#suggestions");
                        suggestions.empty();
                        if(data.length > 0) {
                            $.each(data, function(index, item){
                                suggestions.append('<li class="suggestion-item" style="padding: 5px; cursor: pointer;" data-url="' + item.url + '">' +
                                                   '<a href="' + item.url + '" style="display: flex; align-items: center;">' +
                                                   '<img src="' + item.imageUrl + '" style="width: 50px; height: 50px; margin-right: 10px;">' +
                                                   item.name + '</a></li>');
                            });
                            suggestions.show();
                        } else {
                            suggestions.hide();
                        }
                    }
                });
            } else {
                $("#suggestions").hide();
            }
        });

        $(document).on('click', '.suggestion-item', function(){
            window.location.href = $(this).data('url');
        });
    });
    </script>


